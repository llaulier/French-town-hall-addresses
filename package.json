{
  "name": "adresse-mairie",
  "version": "1.0.0",
  "description": "api adresse mairie",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsnd --respawn ./src/index.ts",
    "db:init": "docker-compose --env-file .env up -d && docker-compose logs -f",
    "db:init:stage": "docker-compose --env-file .env.stage up -d && docker-compose logs -f",
    "db:init:prod": "docker-compose --env-file .env.prod up -d && docker-compose logs -f",
    "db:migrate": "dotenv -e .env -- npx prisma migrate dev",
    "db:migrate:stage": "dotenv -e .env.stage -- npx prisma migrate deploy",
    "db:migrate:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
    "seed": "tsnd ./database/prisma/seed/index.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.19.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "@types/swagger-ui-express": "^4.1.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prisma": "^2.19.0",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": "14",
    "npm": "6.14"
  },
  "type": "module",
  "prisma": {
    "schema": "database/prisma/schema.prisma"
  }
}
